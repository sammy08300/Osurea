{
  "name": "osurea",
  "version": "2.0.0",
  "type": "module",
  "description": "Area Visualizer for osu! tablet players",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,css,json}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.1",
    "vite": "^6.0.0",
    "vitest": "^2.1.6"
  }
}
